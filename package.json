{
  "name": "authentication",
  "version": "1.0.0",
  "description": "Quick start authentication",
  "main": "index.js",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon --experimental-modules server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "db": "npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all"
  },
  "keywords": [
    "node",
    "react",
    "express",
    "sequelize",
    "mysql",
    "jwt"
  ],
  "author": "Dung Nguyen Duong",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
